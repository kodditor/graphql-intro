generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model authors {
  id    String   @id @default(auto()) @map("_id") @db.ObjectId
  books String[]
  id_   String   @map("id")
  name  String
}

model books {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  author String
  id_    String @map("id")
  title  String
}
